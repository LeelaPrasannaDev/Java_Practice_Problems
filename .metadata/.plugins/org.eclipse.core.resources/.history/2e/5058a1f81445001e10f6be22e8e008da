package com.kodNest.Patterns;

public class DemoPattern {

	public static void main(String[] args) {
		
		for(int i=1;i<=2;i++)
		{
			for(int j=1;j<=3;j++)
			
				{
				if(i+j==3||j-i==1)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
				}
			System.out.println();
			}
		}
	
}
