package com.kodNest.Patterns;

public class DemoPattern {

	public static void main(String[] args) {
		int num=3;
		for(int i=1;i<=3;i++)
		{
			for(int j=1;j<=3;j++)
			{
				if(i+j>=4)
				{
					System.out.print("*");
				}
				else
				{
					System.out.println(" ");
				}
			}
			System.out.println();
		}
	}

}
