package com.kodnest.anagram;

public class CheckAnagramOrNot {
	public void  anagramCheckMethod(String inputString1,String inputString2) 
	{
		char []crr =sortMethod(inputString1);
		char []drr=sortMethod(inputString2);
		
		
		
	}

	private char[] sortMethod(String tempString) 
	{
		char [] brr = tempString.toCharArray();
		char temp;
		for(int i=0;i<brr.length;i++)
		{
			for(int j=i+1;j<brr.length;j++)
			{
				if(brr[i]>brr[j])
				{
					temp = brr[i];
					brr[i]=brr[j];
					brr[j]=temp;
				}
			}
		}
		return brr;
	}

	
}
