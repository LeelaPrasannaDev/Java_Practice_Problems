package com.kodNest.Patterns;

public class DemoPattern {

	public static void main(String[] args) {
		
		for(int i=1;i<=10;i++)
		{
			for(int j=1;j<=10;j++)
			{
				if(i+j>=5 && i-j<=4 && j-i<=4 && i+j<=15)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
