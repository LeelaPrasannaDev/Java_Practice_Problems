package com.kodNest.Patterns;

public class DemoPattern {

	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=5;j++)
			
				{
				if(i+j==5||j-i==3||i-j==3||i+j==11)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
				}
			System.out.println();
			}
		}
	
}
